{
  "name": "New York Times",
  "version": "0.1.0",
  "description": "Read articles without a paywall.",
  "content_scripts": [{
    "matches": [
      "*://*.nytimes.com/*"
    ],
    "all_frames": true,
    "run_at": "document_start",
    "js": ["content.js"]
  }],
  "icons": {
     "16": "img/icon16.png",
     "48": "img/icon48.png",
     "55": "img/icon55.png",
     "64": "img/icon64.png"
  },
  "manifest_version": 2
}
